<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.hometech.tech.TechHomeFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_tech_home" />



    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.hometech.tech.TechVideoFragment"
        android:label="Add video"
        tools:layout="@layout/fragment_tech_video" />
    <fragment
        android:id="@+id/navigation_shopping"
        android:name="com.example.hometech.tech.techShopping.ShoppingFragment"
        android:label="@string/title_shopping"
        tools:layout="@layout/fragment_shopping" />
    <fragment
        android:id="@+id/navigation_feed"
        android:name="com.example.hometech.tech.TechfeedFragment"
        android:label="View Feedback"
        tools:layout="@layout/fragment_techfeed" />

</navigation>